---
import { contactMeData } from "@/components/home/data/contactMeData";
import SocialButton from "./SocialButton.astro";
---

<div
  class="mt-15 flex justify-start items-center gap-8"
  data-aos="fade-up"
  data-aos-duration="1400"
>
  <p class="text-lg font-bold">Contact me:</p>
  <div class="flex justify-start gap-2">
    {contactMeData.map((d) => <SocialButton {...d} />)}
  </div>
</div>
